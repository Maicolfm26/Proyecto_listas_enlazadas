<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="352.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.listas.vistas.ControladorVistaPrincipal">
   <children>
      <JFXTabPane fx:id="tabPane" layoutY="-3.0" prefHeight="41.0" prefWidth="380.0">
         <tabs>
            <Tab />
            <Tab onSelectionChanged="#cargarContenedor" text="Cargar contenedor" />
            <Tab onSelectionChanged="#guardarContenedor" text="Guardar Contenedor" />
         </tabs>
      </JFXTabPane>
      <TableView fx:id="tablaContenedor" layoutX="44.0" layoutY="76.0" prefHeight="200.0" prefWidth="293.0">
        <columns>
          <TableColumn fx:id="identificacionColumn" prefWidth="292.0" text="Identificacion del contenedor" />
        </columns>
      </TableView>
      <HBox layoutX="82.0" layoutY="301.0" spacing="50.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#eliminarContenedor" style="-fx-background-color: #ea1313;" text="Eliminar" textFill="WHITE" />
            <Button mnemonicParsing="false" onMouseClicked="#crearContenedor" style="-fx-background-color: #49bb47;" text="Crear contenedor" textFill="WHITE" />
         </children>
      </HBox>
   </children>
</AnchorPane>
